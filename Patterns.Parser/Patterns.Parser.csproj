<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <LangVersion>8.0</LangVersion>
		<Nullable>enable</Nullable>
    <RootNamespace>Stringier.Patterns.Parser</RootNamespace>
    <AssemblyName>Stringier.Patterns.Parser</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Patrick Kelly</Authors>
    <Version>2.2.0</Version>
    <Copyright>2019</Copyright>
    <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Entomy/Stringier</PackageProjectUrl>
    <PackageIcon>Logo.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/Entomy/Stringier</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>Implements a parser for Stringier's Patterns. This is meant for internal use, as it only parses a generalized format of Stringier's Pattern expressions.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Patterns\Patterns.csproj" />
  </ItemGroup>

</Project>
