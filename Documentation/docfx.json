{
  "metadata": [
    {
      "src": [
        {
          "files": [ "*" ],
          "src": "../Collections"
        },
        {
          "file": [ "*" ],
          "src":  "../Core"
        },
        {
          "files": [ "*" ],
          "src": "../Extensions"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns/Bindings"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns/Collections"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns/Errors"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns/Exceptions"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns/Nodes"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns.MSTest"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns.NUnit"
        },
        {
          "files": [ "*" ],
          "src": "../Patterns.Tracing"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}