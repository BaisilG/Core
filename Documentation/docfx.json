{
  "metadata": [
    {
      "src": [
				{
					"files": [ "*" ],
					"src": "../Stringier"
				},
				{
					"files": [ "*" ],
					"src": "../Stringier.Patterns"
				},
				{
					"files": [ "*" ],
					"src": "../Stringier.Patterns/Bindings"
				},
				{
					"files": [ "*" ],
					"src": "../Stringier.Patterns/Collections"
				},
				{
					"files": [ "*" ],
					"src": "../Stringier.Patterns/Exceptions"
				},
				{
					"files": [ "*" ],
					"src": "../Stringier.Patterns.Testing"
				}
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}